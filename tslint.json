{
  "extends": "@knisterpeter/standard-tslint",
  "rules": {
    "space-before-function-paren": [
      true,
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always",
        "method": "never",
        "constructor": "never"
      }
    ],
    "no-parameter-properties": false
  }
}
